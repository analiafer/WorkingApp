<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.workingapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>

    <application
        android:name=".Working"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.WorkingApp"
        android:usesCleartextTraffic="true">

        <activity
            android:name=".ui.SignUpActivity"/>
        <activity
            android:name=".ui.LogInActivity"/>
        <activity
            android:name=".ui.MainActivity"/>
        <activity
            android:name=".ui.UserActivity"
            android:parentActivityName=".ui.MainActivity" />
        <activity
            android:name=".ui.PronosticoActivity"
            android:parentActivityName=".ui.ClimaActivity" />
        <activity
            android:name=".ui.EnProcesoActivity"
            android:parentActivityName=".ui.MainActivity" />
        <activity
            android:name=".ui.CancelActivity"
            android:parentActivityName=".ui.MainActivity" />
        <activity
            android:name=".ui.NewTicketActivity"
            android:parentActivityName=".ui.MainActivity" />
        <activity
            android:name=".ui.EditActivity"
            android:parentActivityName=".ui.MainActivity" />
        <activity
            android:name=".ui.ClimaActivity"
            android:parentActivityName=".ui.MainActivity" />
        <activity
            android:name=".ui.DoneActivity"
            android:parentActivityName=".ui.MainActivity" />
        <activity
            android:name=".ui.ViewTicketActivity"
            android:parentActivityName=".ui.MainActivity" />
        <activity android:name=".ui.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>